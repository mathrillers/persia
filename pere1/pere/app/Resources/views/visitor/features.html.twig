    <!-- Owl Carousel JavaScript -->
    <script src="{{ asset('assets/js/owl.carousel.min.js')}}"></script>

    <!-- Counter JavaScript -->
    <script src="{{ asset('assets/js/waypoints.min.js')}}"></script>
    <script src="{{ asset('assets/js/jquery.counterup.min.js')}}"></script>


    <!-- Script to Activate the Carousel -->
    <script type="text/javascript">
        $(document).ready(function () {
            'use strict';
            $("#pag-carousel").owlCarousel({
                items: 5,
                itemsDesktop: [1199, 5],
                itemsDesktopSmall: [979, 4],
                itemsTablet: [768, 3],
                itemsMobile: [479, 2],
                navigation: true,
                navigationText: [
                    "<i class='fa fa-chevron-left icon-white'></i>",
                    "<i class='fa fa-chevron-right icon-white'></i>"
                ],
                autoPlay: false,
                pagination: false
            });

            // Script to Activate the Counters

            $('.counter').counterUp({
                delay: 50,
                time: 5000
            });

            // Script to Activate the Progress Bars

            $('.progress-bar span').counterUp({
                delay: 10,
                time: 3000
            });
            var progrs = $('.progress-bar');

            $(window).scroll(function () {
                var y = $(this).scrollTop();
                var z = $('#prog_sec').offset().top;

                if (y >= z) {
                    progrs.removeClass('blnk').addClass('profill');
                } else {
                    progrs.removeClass('profill').addClass('blnk');
                }
            });

            $('ul.drop_menu [data-toggle=dropdown]').on('click', function (event) {
                event.preventDefault();
                event.stopPropagation();
                $(this).parent().siblings().removeClass('open');
                $(this).parent().toggleClass('open');
            });

        });
    </script>
{% endblock %}
