{% extends 'admin/layout.html.twig' %}

{% block body %}
    {{ parent() }}
    <body>
    <!-- container section start -->
    <section id="container" class="">

        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-table"></i> Liste des Utilisateurs</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="{{ path('index_page') }}">Home</a></li>
                            <li><i class="fa fa-table"></i>Utilisateurs</li>
                            <li><i class="fa fa-th-list"></i>Liste Utilisateur</li>
                        </ol>
                    </div>
                </div>
                <!-- page start-->


                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">

                            <header class="panel-heading" style="text-align: center;border-top-style: solid;"><br>

                                <div class="col-lg-10">Tableau des Utilisateurs</div>


                                <div class="col-lg-2"><a class="btn btn-success" href="{{ path('utilisateur_new') }}">Nouveau
                                        Utilisateur</a></div>
                                <br><br>
                            </header>
                            {% if personnes %}
                                <div class="table-responsive">
                                    <table class="table table-striped table-advance table-hover table-bordered table-condensed table-body-center"
                                           style="border-radius: 25px;">
                                        <tbody>
                                        {#  <tr>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_profile"></i> Full Name</th>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_calendar"></i> Date</th>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_mail_alt"></i> Email</th>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_pin_alt"></i> City</th>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_mobile"></i> Mobile</th>
                                             <th style="text-align: center;border-style: groove;"><i class="icon_cogs"></i> Action</th>
                                          </tr>#}
                                        <tr style="text-align: center;border-style: groove;">

                                            <th style="text-align: center;border-style: groove;">Nom Complet</th>
                                            <th style="text-align: center;border-style: groove;">Email</th>
                                            <th style="text-align: center;border-style: groove;">Telephone</th>
                                            <th style="text-align: center;border-style: groove;">Adresse</th>
                                            <th style="text-align: center;border-style: groove;">Datenaissance</th>
                                            <th style="text-align: center;border-style: groove;">Pays</th>
                                            <th style="text-align: center;border-style: groove;">Ville</th>

                                            <th style="text-align: center;border-style: groove;">Actions</th>
                                        </tr>
                                        {% for personne in pagination %}
                                            <tr style="text-align: center;border-style: groove;">

                                                <td style="text-align: center;border-style: groove;">{{ personne.nom }}  {{ personne.prenom }}</td>
                                                <td style="text-align: center;border-style: groove;">{{ personne.email }}</td>
                                                <td style="text-align: center;border-style: groove;">{{ personne.telephone }}</td>
                                                <td style="text-align: center;border-style: groove;">{{ personne.adresse }}</td>
                                                <td style="text-align: center;border-style: groove;">{% if personne.dateNaissance %}{{ personne.dateNaissance|date('j F Y') }}{% endif %}</td>
                                                <td style="text-align: center;border-style: groove;">{{ personne.pays }}</td>
                                                <td style="text-align: center;border-style: groove;">{{ personne.ville }}</td>

                                                <td style="text-align: center;border-style: groove;">
                                                    <div class="btn-group">
                                                        <a class="btn btn-primary " style="width: 90px"
                                                           href="{{ path('utilisateur_show', { 'id': personne.personneId }) }}">Afficher</a>
                                                    </div>
                                                    <br><br>

                                                    <div class="btn-group"><a class="btn btn-warning "
                                                                              style="width: 90px"
                                                                              href="{{ path('utilisateur_edit', { 'id': personne.personneId }) }}">Modifier</a>

                                                    </div>
                                                    <br><br>

                                                    <div class="btn-group"><a class="btn btn-danger  "
                                                                              style="width: 90px"
                                                                              href="{{ path('utilisateur_delete', { 'id': personne.personneId }) }}">DÃ©sactiver</a>

                                                    </div>

