{% extends 'admin/layout.html.twig' %}

{% block body %}
{{ parent() }}
<body>
<!-- container section start -->
<section id="container" class="">

    <section id="main-content">
        <section class="wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h3 class="page-header"><i class="fa fa-table"></i> Nouvelle Agence</h3>
                    <ol class="breadcrumb">
                        <li><i class="fa fa-home"></i><a href="{{ path('index_page') }}">Home</a></li>
                        <li><i class="fa fa-table"></i>Agences</li>
                        <li><i class="fa fa-th-list"></i>Nouvelle Agence</li>
                    </ol>
                </div>
            </div>
            <!-- page start-->


            <a class="btn btn-navbar" href="{{ path('agencee_index') }}">Liste des Agences</a>


            <!--***********************************************************************************-->
            <!-- Form validations -->
            <div class="row">
                <div class="col-lg-12">
                    <section class="panel">
                        <header class="panel-heading">
                            Cree Une Nouvelle Agence
                        </header>
                        <div class="panel-body">
                            <div class="form">

                                {{ form_start(form,{'id':'feedback_form','method':'post','attr':{'class':'form-validate form-horizontal'}}) }}
                                <div class="form-group" style="text-align: center">


                                    <div class="col-lg-6">   {{ form_label(form.nom) }}</div>
                                    <div class="col-lg-4">  {{ form_widget(form.nom) }}
                                    </div>


                                </div>
                                <div class="form-group" style="text-align: center">


                                    <div class="col-lg-6"> {{ form_label(form.adresse) }}  </div>
                                    <div class="col-lg-4">   {{ form_widget(form.adresse) }}
                                    </div>


                                </div>
                                <div class="form-group" style="text-align: center">


                                    <div class="col-lg-6"> {{ form_label(form.telephone) }}  </div>
                                    <div class="col-lg-4">   {{ form_widget(form.telephone) }}
                                    </div>


                                </div>
                                <div class="form-group" style="text-align: center">


                                    <div class="col-lg-6">  {{ form_label(form.email) }}  </div>
                                    <div class="col-lg-4">
                                        {{ form_widget(form.email) }}
                                    </div>


                                </div>

                                <div class="form-group" style="text-align: center">


                                    <div class="col-lg-6">  {{ form_label(form.description) }}  </div>
                                    <div class="col-lg-4">
                                        {{ form_widget(form.description) }}
                                    </div>

                                </div>

                                <div class="form-group" style="text-align: center">

                                    <div class="col-lg-6">  {{ form_label(form.numeroCb) }}  </div>
                                    <div class="col-lg-4">
                                        {{ form_widget(form.numeroCb) }}
                                    </div>

                                </div>




                                <div class="form-group">
                                    <div class="col-lg-offset-2 col-lg-10">
                                     &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                        <button class="btn btn-primary" type="submit">Save</button>
                                        <button class="btn btn-default" type="reset">Cancel</button>
                                    </div>
                                </div>
                                {{ form_end(form) }}

                            </div>
                        </div>
                    </section>
                </div>
            </div>

            <!-- page end-->
        </section>
    </section>
    <!--main content end-->
</section>
<!-- container section end -->
{% endblock %}


